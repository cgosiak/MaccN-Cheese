
<expression> -> <expressionStart><expressionEnd>

<expressionStart> -> <term>

<expressionEnd> -> + <term>
                                 | - <term>

<term> -> <termStart><termEnd>

<termStart> -> <factor>

<termEnd> -> * <factor>
                      | / <factor>

<factor> -> ID
                 | INT_LITERAL
                 | FLOAT_LITERAL 
                 | (<expression>)

